{
  "expo": {
    "name": "React Native Firebase Login Template",
    "slug": "react-native-firebase-login-template-typescript",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enzle"
    },
    "description": "Firebase Login Template for React Native apps.",
    "githubUrl": "https://github.com/venits/react-native-firebase-login-screen",
    "android": {
      "package": "com.enzle"
    }
  }
}
